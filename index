    <iframe id="libraryFrame" src="https://js.do/qswwswsd/idklol" title="Byramhills Library"></iframe>
    <button id="switchButton" onclick="switchLibrary()"></button>

    <script>
        function switchLibrary() {
            const iframe = document.getElementById('libraryFrame');
            if (iframe.src !== "https://js.do/qswwswsd/idklol") {
                iframe.src = "https://js.do/qswwswsd/idklol";
            }
        }
    </script>
